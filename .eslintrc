{
    "extends": [
        "@hapi/eslint-config-hapi"
    ],
    "plugins": [
        "@hapi/eslint-plugin-hapi"
    ],
    "rules": {
        "require-await": "off",
        "no-unused-vars": "off",
        "quote-props": [ "error", "consistent-as-needed", { "unnecessary" : true, "numbers" : true }]
    },
    "parserOptions": {
        "ecmaVersion": 2018
    }
}
